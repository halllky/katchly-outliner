<Katchly>

  <Row is="write-model">
    <ID is="uuid"/>

    <!-- 本当はref-to:Rowとしたいがnijoが自身への参照をまだサポートしていない -->
    <Parent is="word" />

    <Label is="sentence" />
    <RowType is="ref-to:RowType"/>
    <Attrs is="array">
      <ColType is="ref-to:RowType/Columns key" />
      <Value is="sentence" />
    </Attrs>
  </Row>

  <RowType is="write-model">
    <ID is="uuid"/>
    <Columns is="array">
      <ColumnId is="uuid" />
      <ColumnName is="word" />
    </Columns>
  </RowType>

</Katchly>
